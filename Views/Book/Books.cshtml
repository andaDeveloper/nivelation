@model List<BookModel>

@{
    ViewData["Title"] = "Library";
}
<h1>@ViewData["Title"]</h1>



<table class="table">
    <thead>
        <tr>
            <th>Título</th>
            <th>Autor</th>
            <th>Fecha de lanzamiento</th>
            <th>Campos de autodítoria</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var book in Model)
        {
            <tr>
                <td>@book.Nombre_libro</td>
                <td>@book.Autor</td>
                <td>@book.Fecha_lanzamiento</td>
                <td>@book.Campos_auditoria</td>
                <td><a href="view/@book.Id_libro">Update</a></td>
            </tr>
        }
    </tbody>
</table>
